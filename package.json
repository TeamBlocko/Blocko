{
	"name": "blocko",
	"version": "1.8.0",
	"description": "",
	"main": "index.js",
	"dependencies": {
		"@rbxts/compiler-types": "^1.0.0-beta.16.1",
		"@rbxts/flipper": "^1.2.1",
		"@rbxts/net": "^1.3.0-beta.6.2",
		"@rbxts/number-manipulator": "^1.1.2",
		"@rbxts/object-utils": "^1.0.4",
		"@rbxts/profileservice": "^1.2.2",
		"@rbxts/roact": "^1.3.0-ts.6",
		"@rbxts/roact-dnd": "0.0.6",
		"@rbxts/roact-rodux": "^0.1.2",
		"@rbxts/rodux": "^1.1.0",
		"@rbxts/ser": "^1.2.1",
		"@rbxts/services": "^1.1.2",
		"@rbxts/t": "^2.1.1"
	},
	"scripts": {
		"rojo": "rojo serve",
		"rojo:packed": "rojo serve packed.project.json",
		"rojo:build": "rojo build -o dist/WorldTemplate.rbxlx",
		"rojo:build:packed": "rojo build -o dist/WorldTemplate.Packed.rbxlx packed.project.json",
		"remodel": "remodel run add-models.lua",
		"remodel:packed": "remodel run add-models.lua packed",
		"compile": "rbxtsc",
		"precompile": "npm run project:version",
		"build": "npm run rojo:build && npm run remodel",
		"build:packed": "npm run rojo:build:packed && npm run remodel:packed",
		"project:build": "npm run compile && npm run build",
		"project:serve": "npm run compile && npm run rojo",
		"project:build:packed": "npm run compile && npm run build:packed",
		"eslint:fix": "eslint src --fix",
		"project:version": "echo VERSION=$(git describe --tags --first-parent --long --always) | tee .env"
	},
	"devDependencies": {
		"@rbxts/types": "^1.0.459",
		"@typescript-eslint/eslint-plugin": "^4.15.2",
		"@typescript-eslint/parser": "^4.15.2",
		"eslint": "^7.20.0",
		"eslint-config-prettier": "^6.15.0",
		"eslint-plugin-prettier": "^3.3.1",
		"eslint-plugin-roblox-ts": "0.0.24",
		"prettier": "^2.2.1",
		"rbxts-transform-env": "^0.3.0",
		"rbxts-transformer-t": "^1.1.3",
