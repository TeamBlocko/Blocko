{
	"name": "Blocko World Template",
	"tree": {
		"$className": "DataModel",
		"ServerStorage": {
			"$className": "ServerStorage",
			"Resources": {
				"$className": "Folder",
				"ServerScriptService": {
					"$className": "Folder",
					"TS": {
						"$path": "out/template/server"
					}
				},
				"ReplicatedStorage": {
					"$className": "Folder",
					"TS": {
						"$path": "out/template/shared"
					}
				},
				"StarterPlayer": {
					"$className": "Folder",
					"StarterPlayerScripts": {
						"$className": "Folder",
						"TS": {
							"$path": "out/template/client"
						}
					}
				}
			}
		},
		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
			"rbxts_include": {
				"$path": "include",
				"node_modules": {
					"$path": "node_modules/@rbxts"
				}
			}
		},
		"Workspace": {
			"$className": "Workspace",
			"$properties": {
				"FilteringEnabled": true
			}
		},
		"SoundService": {
			"$className": "SoundService",
			"$properties": {
				"RespectFilteringEnabled": true
			}
		},
		"Chat": {
			"$className": "Chat",
			"$properties": {
				"LoadDefaultChat": false
			}
		}
	},
	"globIgnorePaths": [
		"**/package.json"
	]
}